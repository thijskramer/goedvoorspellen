<div class="venuelist">
    <div class="row">
        <% @venues.each_with_index do |venue, index| %>
        <div class="col-md-4 col-sm-6 col-xs-12">
            <a class="venue" href="<%= venue_path(venue) %>">
              <div class="logo"><%= image_tag venue.picture(:thumb) %></div>
              <h2><%= venue.name %></h2>
              <span class="subtitle"><%= venue.city %></span>
            </a>
        </div>
        <% end %>
    </div>
</div>